<div class="form-group">
  <%= builder.label :user_id, 'Instructor' %>
  <%= builder.select :user_id, options_for_select(User.all.collect { |user| [user.full_name, user.id] }), {}, {class: 'form-control'} %>
</div>

<div class="form-group">
  <%= builder.label :course_status_id, 'Status' %>
  <%= builder.select :course_status_id, User.admin_course_status.all.collect { |status| [status.name, status.id]}, {include_blank: '-- Select One --'}, {class: 'form-control'} %>
</div>